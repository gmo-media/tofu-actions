{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Do not pin digests of own action",
      "matchManagers": ["github-actions"],
      "matchDepNames": [
        "gmo-media/tofu-actions/*",
      ],
      "pinDigests": false,
    }
  ]
}
